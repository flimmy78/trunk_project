main() {
        local version=$1

        version_check ${version} || {
                echo "bad version:${version}"
                return 1
        }

        local current=$(pwd); current=${current#/home/}
        local path_make_lock=/tmp/${current}; mkdir -p ${path_make_lock}
        local file_make_lock=${path_make_lock}/.make_lock

        exec_with_flock ${file_make_lock} do_make ${version}
        rm -f ${hisitopdir}/version
}