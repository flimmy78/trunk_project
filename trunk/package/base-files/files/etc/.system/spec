config spec cpu
	option type		'apm,apm866xx'

config spec flash
	option type		'WINBOND W25Q128BV16'
	option count	1
	option size		16M

config spec memory
	option type		'K4B2G1646C-HCH9'
	option count	1
	option size		64M

config spec eth
	option base		0
	option count		1

config spec wifi
	option base		0
	option count		3

config spec eth0
	option up_speed		1000

config spec eth1
	option up_speed		1000

config spec wifi0
	option hwmode		bgn
	option minpower2g	0
	option maxpower2g	20
config spec wifi1
	option hwmode		bgn
	option minpower2g	0
	option maxpower2g	10
	option minpower5g	0
	option maxpower5g	10
config spec wifi2
	option hwmode		bgn
	option minpower2g	0
	option maxpower2g	10
	option minpower5g	0
	option maxpower5g	10
